{
  "name": "node-gpx-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app/bin/www",
    "pcopy": "cp plist/orion/nl.jcroonen.gpxserver.plist ~/Library/LaunchAgents",
    "pstart": "launchctl start nl.jcroonen.gpxserver",
    "pstop": "launchctl stop nl.jcroonen.gpxserver",
    "pload": "launchctl load -w ~/Library/LaunchAgents/nl.jcroonen.gpxserver.plist",
    "punload": "launchctl unload ~/Library/LaunchAgents/nl.jcroonen.gpxserver.plist",
    "plist": "launchctl list | grep jcroonen"
  },
  "dependencies": {
    "acorn": ">=5.7.4",
    "chokidar": "^3.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.2",
    "csv-writer": "^1.6.0",
    "debug": "~2.6.9",
    "easy-fit": "0.0.8",
    "express": "~4.16.1",
    "gpxparser": "^3.0.8",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-fetch": "^3.3.1",
    "plist": "^3.0.6",
    "pug": "^2.0.4",
    "xml2js": "^0.4.23"
  }
}
